{
  "name": "@mongodb-js/charts-embed-dom",
  "version": "1.1.3",
  "description": "JavaScript library for embedding MongoDB Charts",
  "license": "Apache-2.0",
  "main": "dist/charts-embed-dom.cjs.js",
  "module": "dist/charts-embed-dom.esm.js",
  "unpkg": "dist/charts-embed-dom.umd.min.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "docs:check": "ts-node ./bin/lint-src-for-docs.ts",
    "start": "start-storybook -p 3002",
    "test": "tsc --skipLibCheck --noEmit && NODE_OPTIONS='--experimental-worker' mocha --ui bdd -r jsdom-global/register -r ./mocha-setup.js \"./src/**/*.spec.ts\" && :",
    "build:umd": "webpack --mode production",
    "release:npm": "yarn docs && ./bin/publish-to-npm-if-version-changed"
  },
  "repository": "https://github.com/mongodb-js/charts-embed-sdk",
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@leafygreen-ui/palette": "^1.1.0",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/html": "^5.2.5",
    "@storybook/theming": "^5.2.5",
    "@types/chai": "^4.2.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "csstype": "^2.6.7",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.0-3",
    "typescript": "^3.8.3",
    "webpack": "^4.35.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@looker/chatty": "^2.2.0",
    "@types/bson": "^4.0.1",
    "bson": "^4.0.2",
    "jsonwebtoken": "^8.5.1"
  }
}
